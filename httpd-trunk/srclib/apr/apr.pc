prefix=/usr/local2/apache2
exec_prefix=/usr/local2/apache2
libdir=${prefix}/lib
APR_MAJOR_VERSION=1
includedir=/usr/local2/apache2/include

Name: APR
Description: The Apache Portable Runtime library
Version: 1.4.7
Libs: -L${libdir} -lapr-${APR_MAJOR_VERSION} -lpthread
Cflags: -DDARWIN -DSIGPROCMASK_SETS_THREAD_MASK -no-cpp-precomp -DDARWIN_10 -g -O2 -Wall -Wmissing-prototypes -Wstrict-prototypes -Wmissing-declarations -I${includedir}
